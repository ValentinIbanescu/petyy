<div class="container">
  <h1> Book this pet!</h1>
  <div class = "container text-left">
    <%= simple_form_for [@pet, @booking] do |f| %>
      <%= f.input :start_date, html5: true %>
      <%= f.input :end_date, html5: true %>
      <div class="d-flex justify-content-center">
        <%= f.submit 'Book', class: "btn-flat", id: "btn-book" %>
      </div>
    <% end %>
  </div>

</div>
